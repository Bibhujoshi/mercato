<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/1.5/distro-source/core/src/gwt-module.dtd">
<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.user.History"/>
	<inherits name="com.google.gwt.i18n.I18N"/>
	
	<extend-property name="locale" values="en_US"/>
	
	<set-property name="user.agent" value="gecko"/>
	
<!--	<inherits name="org.gwtwidgets.WidgetLibrary"/>-->
	<!-- Inherit the smartGwt Toolkit library configuration -->
    <inherits name="com.smartgwt.SmartGwt"/>
    <inherits name="net.sf.gilead.Adapter4Gwt15"/>
    <inherits name="org.gwtwidgets.WidgetLibrary"/>

    
    <!-- Inherit the smartGwt skins -->
	<inherits name="com.smartclient.theme.enterprise.EnterpriseResources"/>    
    <inherits name="com.smartclient.theme.silverwave.SilverWaveResources"/>    
    <inherits name="com.smartclient.theme.fleet.FleetResources"/>
  
	<!-- Specify the app entry point class.                   -->
	<entry-point class="com.alten.mercato.client.MercatoMain"/>
  	
  	<source path="server/model"/>
  	<source path="client"/>
  	
</module>